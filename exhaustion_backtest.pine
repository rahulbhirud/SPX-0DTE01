f_index_of_time(int[] times, int t) =>
    array.binary_search(times, t)

bullIndex = f_index_of_time(bullTimes, time)
bearIndex = f_index_of_time(bearTimes, time)

bullStrength = bullIndex >= 0 ? array.get(bullStrengths, bullIndex) : na
bearStrength = bearIndex >= 0 ? array.get(bearStrengths, bearIndex) : na

plotshape(bullIndex >= 0 and bullStrength >= 5, title="Bull Exhaustion (Strong)", style=shape.labeldown, location=location.abovebar, color=color.new(color.red, 0), textcolor=color.white, size=size.large, text="BU")
plotshape(bullIndex >= 0 and bullStrength < 5, title="Bull Exhaustion", style=shape.labeldown, location=location.abovebar, color=color.new(color.red, 0), textcolor=color.white, size=size.small, text="BU")
plotshape(bearIndex >= 0 and bearStrength >= 5, title="Bear Exhaustion (Strong)", style=shape.labelup, location=location.belowbar, color=color.new(color.green, 0), textcolor=color.white, size=size.large, text="BE")
